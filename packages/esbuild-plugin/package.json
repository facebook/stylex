{
  "name": "@stylexjs/esbuild-plugin",
  "version": "0.6.1",
  "description": "StyleX esbuild plugin",
  "main": "lib/index.js",
  "type": "module",
  "repository": "https://github.com/facebook/stylex.git",
  "license": "MIT",
  "scripts": {
    "prebuild": "gen-types -i src/ -o /lib",
    "build": "babel src/ --out-dir lib/ --copy-files",
    "test": "jest"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/plugin-syntax-typescript": "^7.23.3",
    "@stylexjs/babel-plugin": "^0.6.1",
    "@stylexjs/shared": "^0.6.1",
    "babel-plugin-syntax-hermes-parser": "^0.21.1",
    "esbuild": "^0.19.12"
  },
  "devDependencies": {
    "@stylexjs/stylex": "^0.6.1",
    "eslint": "^8.55.0"
  }
}
